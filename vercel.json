{
    "projects": [
      {
        "name": "frontend",
        "src": "frontend/",
        "buildCommand": "npm run build",
        "outputDirectory": "frontend/.next"
      },
      {
        "name": "backend",
        "src": "backend/",
        "buildCommand": "npm run build",
        "outputDirectory": "backend/build"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/backend/$1"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "frontend/$1"
      }
    ]
  }
  